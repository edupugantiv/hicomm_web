<%=render partial: 'management'%>
<div id="section">
<h1>Add Members</h1>
<%if @users.empty? %> 
	<i> There are no users to add to your group at this time</i>
<%else%>
	<ol><%@users.each do |user|%>
		<li><%=link_to user.name, user_path(user)%> <%=button_to "Add", add_group_members_path(:group_id => @group, :user_id => user), :method => 'put' %></li>
	<%end %></ol>
<%end%>
</div>